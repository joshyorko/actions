{
  "name": "sema4ai-action-server-frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --minify=esbuild --outDir=./dist",
    "build:debug": "vite build -m=debug --minify=false --outDir=./dist",
    "build:tests": "vite build -m=debug --minify=false --outDir=./dist-test",
  "build:size": "node ./scripts/check-bundle-size.cjs",
    "test": "vitest --run",
    "test:visual": "playwright test",
    "test:lint": "eslint --ext .tsx,.ts --max-warnings 0 .",
    "test:types": "tsc --noEmit",
    "test:prettier": "prettier --check ./src/**/*.{ts,tsx}",
    "test:a11y": "vitest --run __tests__/a11y --threads=1",
    "prettier": "prettier --write ./src/**/*.{ts,tsx}",
    "fixlint": "eslint --fix --ext .tsx,.ts ."
  },
  "dependencies": {
    "@codemirror/commands": "^6.6.0",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/language": "^6.10.0",
    "@codemirror/legacy-modes": "^6.3.3",
    "@codemirror/view": "^6.28.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.28.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^2.30.0",
    "openapi-types": "^12.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "^4.0.3",
    "@playwright/test": "^1.42.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.17",
    "axe-core": "^4.11.0",
    "eslint": "^8.56.0",
    "http-server": "^14.1.1",
    "jsdom": "^21.1.0",
    "lhci": "^0.10.2",
    "postcss": "^8.4.33",
    "prettier": "^3.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^6.1.0",
    "vite-plugin-singlefile": "^2.1.0",
    "vitest": "^3.1.3",
    "@cyclonedx/cyclonedx-npm": "^4.0.3"
  },
  "prettier": "prettier"
}
